R_A		d2	d1	1E12
Vgate_s		g3	g2	0
Vdrain_s	d3	d2	0
Gheat		0	Tj	value = {abs((V(d,s)*I(Vdrain_s)))+abs((V(g1,g2)*I(Vgate_s)))}
XCGD		d1	g1	cgdmos params: af=af1
CGS		g1	s1	{1876.384p*af1}
CDS		d2	s1	{60p*af1}
D1		s1	d2	bodydiode
