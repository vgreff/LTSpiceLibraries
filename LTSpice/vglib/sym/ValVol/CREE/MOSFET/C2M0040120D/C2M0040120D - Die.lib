.model bodydiode d(is=1e-4 cjo=573.713p bv=1600 m=0.6 EG=3.26 vj=3.82 n=13.2 
+	rs=0.044  trs1=-0.0046833036 trs2=0.0000171023 Tnom=25
+	tt=15n ibv=500u Xti=40 level=1)
****************************************

.subckt cgdmos d2 g
.param k1=857.672p
.param k2=0.5
.param ka=18.096
.param kb=0.2678
.param kc=7.5277
G11 g d1 value = 	{
+		af*k1*(
+		(1+(limit(v(d1,g),0,500))*(1+ka*(1+TANH(kb*V(d1,g)-kc))/2))**-k2
+		)*ddt(v(g,d1))
+			}
R_CGD d1 d2 1e-3
C_CGD g d1 {1p*af}
.ends cgdmos
