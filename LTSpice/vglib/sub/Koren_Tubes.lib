.SUBCKT HEXODE  1 2 3 4 5 RGI=1Meg ; A G3 G1 C G2 
RE1  7 0  1MEG    ; DUMMY SO NODE 7 HAS 2 CONNECTIONS
BE1   7 0  V=V(2,4)/KP*LOG(1+EXP((1/MU+(V(3,4)+V(5,4))/V(2,4))*KP)) ; E1 BREAKS UP LONG
G1   1 4  VALUE={(PWR(V(7),EX)+PWRS(V(7),EX))/KG1*ATAN(V(1,4)/KVB)}
G2   2 4  VALUE={(EXP(EX*(LOG((V(2,4)/MU)+(V(3,4)+V(5,4))))))/KG2}
*G2   2 4  VALUE={PWR((V(2,4)/MU)+(V(3,4)+V(5,4)), EX )/KG2}
RCP  1 4  1G       ; FOR CONVERGENCE             A  - C
C1   3 4  {CCG1}   ; CATHODE-GRID 1              C  - G1
C2   5 4  {CCG2}   ; CATHODE-GRID 2              C  - G2
C3   1 3  {CPG1}   ; GRID 1-PLATE                G1 - A
C4   1 5  {CPG2}   ; GRID 2-PLATE                G2 - A
C5   1 4  {CCP}    ; CATHODE-PLATE               A  - C
RG1   3 8  {RGI}   ; FOR GRID CURRENT            G1 - 8
DG1   8 4  DX      ; FOR GRID CURRENT            8  - C
RG2   5 9  {RGI}   ; FOR GRID CURRENT            G2 - 9
DG2   9 4  DX      ; FOR GRID CURRENT            9  - C
.MODEL DX D(IS=1N RS=1 CJO=10PF TT=1N) ;
.ENDS HEXODE



******************
*
******************
* (Diode - Child-Langmuirs Law)
*               anode 1
*               | anode 2
*               | | cathode
*               | | |
.SUBCKT DULEREC 1 2 3
B1	1	3	I=if( V(1,3)>0, {G}*(V(1,3)**1.5), 0)
B2	2	3	I=if( V(2,3)>0, {G}*(V(2,3)**1.5), 0)
.ENDS DULEREC

******************
* (Diode - Child-Langmuirs Law)
*           anode
*           | cathode
*           | |
.SUBCKT REC 1 2
B1	1	2	I=if( V(1,2)>0, {G}*(V(1,2)**1.5), 0)
.ENDS REC

******************
* (Diode - Child-Langmuirs Law + contact potential)
*            anode
*            | cathode
*            | |
.SUBCKT REC2 A C
RE	7	0	1MEG    ; DUMMY SO NODE 7 HAS 2 CONNECTIONS
E1	7	0	VALUE={g}*POW(V(a,c)+{Vb},1.5)
G1	a	c	VALUE={if(V(a,c)+{Vb}>0, V(7), 0)}
R1	A	C	1T
.ENDS

